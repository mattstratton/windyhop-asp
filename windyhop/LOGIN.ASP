<%'Option Explicit%><!-- #include file="../include/globalc.inc" --><!-- #include file="../include/liblogin.inc" --><%If Request.QueryString("submit") = "login" Then	CheckLogin()Else	If Session("userid") <> "" Then		%>		<script language = "javascript">		parent.Top_Nav.location='/windyhop/header.asp?tab=<%=Session("Tab")%>';		</script>		<%		'Response.Redirect("side_nav.asp")	End If	If Request.QueryString("invalid") = YES Then		Session("invalidlogin") = YES				%>		<script language = "javascript">		parent.Top_Nav.location='/windyhop/header.asp?tab=<%=Session("Tab")%>';		</script>		<%		'Response.Redirect("side_nav.asp")	Else		If Session("userid") = "" Then			%>			<h1>Login into Windyhop</h1>			Please enter your user and password. <br><br> If you do not have an account on Windyhop, <a href=edituser.asp>get one now.</a><p>			<%					LocalBuildLoginForm()					End If	End IfEnd If%>