var agt=navigator.userAgent.toLowerCase();function isWin() {    if ((agt.indexOf("win") != -1) || (agt.indexOf("16bit") != -1) ||    (agt.indexOf("win16") != -1) || (agt.indexOf("windows 16-bit") != -1) ||    (agt.indexOf("16") != -1) || (agt.indexOf("windows 3.1") != -1) ||        (agt.indexOf("win32") != -1) || (agt.indexOf("32bit") != -1) ||        (agt.indexOf("win95") != -1) || (agt.indexOf("windows 95") != -1) ||        (agt.indexOf("win98") != -1) || (agt.indexOf("windows 98") != -1) ||        (agt.indexOf("winnt") != -1) || (agt.indexOf("windows nt") != -1)) return true;    else return false;}function isMac() {    if ((agt.indexOf("ppc") != -1) || (agt.indexOf("mac") != -1) ||    (agt.indexOf("powerpc") != -1) || (agt.indexOf("68k") != -1) ||    (agt.indexOf("68000") != -1)) return true;    else return false;}function fontSize() {	if (isWin()) {      document.write('<FONT SIZE=\"2\" face=\"Helvetica, Arial\">');    }    else if(isMac()) {      document.write('<FONT SIZE=\"3\" face=\"Helvetica, Arial\">');    }    else {      document.write('<FONT SIZE=\"2\" face=\"Helvetica, Arial\">');    }}function smfontSize() {        if (isWin()) {         document.write('<FONT SIZE=\"1\" face=\"Helvetica, Arial\">');        }        else if(isMac()) {          document.write('<FONT SIZE=\"2\" face=\"Helvetica, Arial\">');        }		else {          document.write('<FONT SIZE=\"1\" face=\"Helvetica, Arial\">');        }}var n;var ie;function getBrowser() {n  = (document.layers) ? 1:0;ie = (document.all)    ? 1:0;}// Need to add to this to define number of tabscontent1=0;content2=0;content3=0;content4=0;content5=0;content6=0;// Netscape Resize Fixif (document.layers)	{    widthCheck = window.innerWidth    heightCheck = window.innerHeight    window.onResize = resizeFix	}function resizeFix()	{    if (widthCheck != window.innerWidth || heightCheck != window.innerHeight) document.location.href = docume