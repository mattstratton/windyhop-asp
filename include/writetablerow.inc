<%''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''Sub WriteTableRow(strTitle, strData, strHeading)		If strHeading = HEADING Then		Response.Write("<tr><td bgcolor=Silver><b>" & strTitle & "</b></td><td bgcolor=Silver><b>" & strData & "</b></td></tr>")	Else		Response.Write("<tr ><td valign=top>" & strTitle & "</td><td>" & strData & "</td></tr>")	End IfEnd Sub''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''%>