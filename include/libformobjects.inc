<%' Library Name: libformobjects.inc'' Functions Included:'     BuildUserLevelRadio(iChecked)'     BuildEventDropDown(iSelected)'     BuildVenueDropDwon(iSelected)'''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''Sub BuildUserLevelRadio(iChecked)				'Response.Write(iChecked)	If iChecked <> "" Then		iChecked = FormatNumber(iChecked,0)	End If		Dim strNoPrivChecked	Dim strPrivChecked	Dim strAdminChecked				strNoPrivChecked = ""	strPrivChecked = ""	strAdminChecked = ""		Select Case iChecked		Case FormatNumber(NOPRIVUSER, 0)			strNoPrivChecked = "CHECKED"		Case FormatNumber(PRIVUSER,0)			strPrivChecked = "CHECKED"		Case FormatNumber(ADMIN,0)			strAdminChecked = "CHECKED"	End Select				Response.Write("<input type=radio name=userlevel value=" & NOPRIVUSER & " " & strNoPrivChecked & ">" & NOPRIVUSERNAME & "<br> ")	Response.Write("<input type=radio name=userlevel value=" & PRIVUSER & " " & strPrivChecked & ">" & PRIVUSERNAME & "<br>")	Response.Write("<input type=radio name=userlevel value=" & ADMIN & " " & strAdminChecked & ">" & ADMINNAME & "<br> ")End Sub 'BuildUserLevelRadio(iChecked)''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''Function BuildEventDropDown(eventSQL, iSelected)		Dim objDB	Dim objRS	Dim strSelected 		Set objDB = Server.CreateObject("ADODB.Connection")	objDB.Open DATABASE		Set objRS = objDB.Execute(eventSQL)		'If objRS.EOF Then	'	Exit Function	'End If		Response.Write("<body bgcolor=white>")	Response.Write("<select name=eventid size=1>")		Response.Write("<option selected value=" & DEFAULT & ">Choose an Event:" )		' Write a NEW record option. 	'If Session("userlevel") = ADMIN then	'	Response.Write("<option  value=0>NEW RECORD" )	'End If		Do While Not objRS.EOF		If objRS("eventid") = iSelected Then			strSelected = "SELECTED"		Else 			strSelected = " "		End If				Response.Write("<option value=" & objRS("eventid") & strSelected & ">" & objRS("name") )		objRS.MoveNext	Loop		objRS.Close	objDB.Close	Set objRS = Nothing	Set objDB = Nothing		Response.Write("</select>")End Function ' BuildEventDropDown()''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''Function BuildDropDown(strTable, localSQL, iSelected)		Dim objDB	Dim objRS	Dim strSelected 	Dim temp	Dim temp2		'temp = 	'Response.Write(iSelected)	If Not IsNumeric(iSelected) Then		iSelected = 0	End If		Set objDB = Server.CreateObject("ADODB.Connection")	objDB.Open DATABASE	'Response.Write(localSQL)	Set objRS = objDB.Execute(localSQL)		'Response.Write("<body bgcolor=white>")	Response.Write("<select name=" & strTable & "id size=1>")		If 	Left(strTable,1) =  "a" or _		Left(strTable,1) =  "e" or _		Left(strTable,1) =  "i" or _		Left(strTable,1) =  "o" or _		Left(strTable,1) =  "A" or _		Left(strTable,1) =  "E" or _		Left(strTable,1) =  "I" or _		Left(strTable,1) =  "O" Then		temp2 = "Choose an"	Else		temp2 = "Choose a"	End If		Response.Write("<option selected value=" & DEFAULT & ">" & temp2 & " " & strTable & ":" )		Do While Not objRS.EOF		If FormatNumber(objRS(strTable & "id")) = FormatNumber(iSelected) Then			strSelected = " SELECTED "		Else 			strSelected = " "		End If				Response.Write("<option value=" & objRS(strTable & "id") & strSelected & ">" & objRS("label") )		objRS.MoveNext	Loop		objRS.Close	objDB.Close	Set objRS = Nothing	Set objDB = Nothing		Response.Write("</select>")End Function ' BuildDropDown()''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''Sub BuildPriorityRadio2(iPriority, strName)	Dim x	Dim sChecked	For x = 1 to 10 		If iPriority = x Then			sChecked = CHECKED		Else			sChecked = ""		End If		Response.Write("<input type=radio value="&x&" name=priority"&strName&" "&sChecked&" >"&x)	NextEnd Sub''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''Sub BuildPriorityRadio(iPriority, strName)	Dim x	Dim sChecked	Response.Write("<select name=priority"&strName&" >")	For x = 1 to 10 		If iPriority = x Then			sChecked = SELECTED		Else			sChecked = " "		End If		Response.Write("<option value=" & x & " " & sChecked & ">" & x )		'Response.Write("<input type=radio value="&x&" name=priority"&strName&" "&sChecked&" >"&x)	Next	Response.Write("</select>")End Sub%>