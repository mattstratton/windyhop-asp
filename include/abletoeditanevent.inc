<%''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''Function OwnerOf(strTable, strRecordID, strUserID)	Dim objRS	Dim objDB	Dim ownSQL		Dim tempRecordID	'Response.Write("tempRecordID = '" & tempRecordID & "'<br>")		If IsNull(strRecordID) = TRUE Then		tempRecordID = DEFAULT	Else		tempRecordID = strRecordID	End If		'tempRecordID = DEFAULT			'Response.Write("tempRecordID = '" & tempRecordID & "'<br>")	Set objDB = Server.CreateObject("ADODB.Connection")	objDB.Open DATABASE	ownSQL = "select userid from " & strTable & " where " & strTable & "id = " & tempRecordID	'Response.Write(ownSQL)	Set objRS = objDB.Execute (ownSQL)		If objRS.EOF = TRUE Then		OwnerOf = FALSE	ElseIf objRS("userid") = strUserID Then 'They own it!		OwnerOf = TRUE	Else		OwnerOf = FALSE	End If		End Function 'OwnerOf''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''Function AbleToEditAnEvent(strUserID, strEventID)	Dim objRS	Dim objRS2	Dim objDB	Dim ownSQL		Set objDB = Server.CreateObject("ADODB.Connection")	objDB.Open DATABASE	ownSQL = "Select userid, eventid, bandid, instructorid, venueid from event where eventid = "&strEventID	Set objRS = objDB.Execute (ownSQL)		'Response.Write("strUserID = "&strUserID&"<br>")	'Response.Write("UserID = "&objRS("userid")&"<br>")		If objRS("userid") = strUserID Then		AbleToEditAnEvent = TRUE	ElseIf OwnerOf("band", objRS("bandid"), strUserID) = TRUE Then		AbleToEditAnEvent = TRUE	ElseIf OwnerOf("venue", objRS("venueid"), strUserID) = TRUE Then		AbleToEditAnEvent = TRUE	ElseIf OwnerOf("instructor", objRS("instructorid"), strUserID) = TRUE Then		AbleToEditAnEvent = TRUE	Else		AbleToEditAnEvent = FALSE	End If	End Function%>