<%Option ExplicitDim strIndexFileDim intWidthIf inStr(Request.ServerVariables("HTTP_USER_AGENT"),"MSIE") Then	intWidth = 138 'used to be 138Else	intWidth = 143End IfIf inStr(Request.ServerVariables("HTTP_HOST"),"csds") Then	Session("Tab") = "CSDS"	strIndexFile = "/csds/aboutus.htm"ElseIf inStr(Request.ServerVariables("HTTP_HOST"),"wclx") OR inStr(Request.ServerVariables("HTTP_HOST"),"exchange")Then	Response.Redirect("/exchange")End If	Select Case Request("Tab")Case "CSDS"	Session("Tab") = "CSDS"	strIndexFile = "/csds/aboutus.htm"Case "SS"	Session("Tab") = "SS"	strIndexFile = "/ssu/index.htm"Case Else	Session("Tab") = "SHC"	strIndexFile = "/shc/welcome.asp"End SelectIf Request.QueryString("page") <> "" Then	strIndexFile = Request.QueryString("page")End IfEnd If%><html><head><title>Windyhop</title></head><frameset rows="108,*" frameborder="no" border="0" framespacing="0"> <!--New row size; used to be 100-->  <frame name="Top_Nav" scrolling="NO" noresize src="/windyhop/header.asp?Tab=<%=Session("Tab")%>" marginwidth="0" marginheight="0" ><frameset cols="<%=intWidth%>,*" frameborder="no" border="0" framespacing="0"> <!--New column size; used to be 138-->  <frame name="Side_Nav" scrolling="AUTO" noresize src="/windyhop/Side_Nav.asp?Tab=<%=Session("Tab")%>" marginwidth="0" marginheight="0">	  <frame name="Main" src="<%=strIndexFile%>" marginwidth="0" marginheight="0"></frameset></frameset><noframes><body bgcolor="#FFFFFF"></body></noframes></html>