				<% If counter = month(datFirstDay) Then %>					<option value="<%= counter %>" SELECTED><%= monthName(counter) %></option>				<% Else  %>					<option value="<%= counter %>"><%= monthName(counter) %></option>				<% End If %>			<% Next %>			</select>		<select name="year">											<%				'BuildDropDown "Event", _				'	" SELECT DISTINCT Year(Event.Date) AS Label, " & _				'	"        DISTINCT Year(Event.Date) as eventid " & _				'	" FROM 	 Event " , Year(date)								%>				<option value="2002" >2003</option>					<option value="2003" selected>2004</option>					</select>																				<input type="submit" value="Go!">			</form>		</td>		<td align=right><a href=editevent.asp?action=New><small>Add New Event</small></a></td>		</tr>		</table>	<table border="1">	<tr>		<td class="weekday" align="center" bgcolor = "black">&nbsp; <font color = "white">Sun</font> &nbsp;</td> 		<td class="weekday" align="center" bgcolor = "black">&nbsp; <font color = "white">&nbsp; Mon</font> &nbsp;</td>		<td class="weekday" align="center" bgcolor = "black">&nbsp; <font color = "white">&nbsp; Tue</font> &nbsp;</td> 		<td class="weekday" align="center" bgcolor = "black">&nbsp; <font color = "white">&nbsp; Wed</font> &nbsp;</td>		<td class="weekday" align="center" bgcolor = "black">&nbsp; <font color = "white">&nbsp; Thu</font> &nbsp;</td> 		<td class="weekday" align="center" bgcolor = "black">&nbsp; <font color = "white">&nbsp; Fri</font> &nbsp;</td>		<td class="weekday" align="center" bgcolor = "black">&nbsp; <font color = "white">&nbsp; Sat</font> &nbsp;</td>	</tr>		<% While intPrintDay <= intLastDay %>		<tr>		<% 		Dim intLoopDay		intIsFirst = 1For intLoopDay = 1 To 7 %><%'Inside the For...Next Loop%><% 'Beginning of a cell%><td ALIGN="CENTER" height="100" width="100" valign="top" class = "weekview"><% ' Check to see if the day is valid bullshitIf intFirstWeekDay > 1 Then %>	-	<% intFirstWeekDay = intFirstWeekDay - 1	Else		If intPrintDay > intLastDay Then %>		-	<% Else %>		<b>		<% If intPrintDay = Day(date()) Then			Response.Write("<a class=highlight>")			Response.Write(intPrintDay)			Response.Write("</a>")		Else		%>		<% = intPrintDay %>		<% End If %>		</b>		<br>		<% If Not objRSEvent.EOF Then			Do While day(objRSEvent("date")) = intPrintDay				If intIsFirst = 0 Then					Response.Write("<hr width = 50 color = gray>")				Else					Response.Write("<br>")				End If								Select Case objRSEvent("EventType")								Case WORKSHOP					Response.Write("<a class = emblem>W</a>&nbsp;")									Case LESSON					Response.Write("<a class = emblem>L</a>&nbsp;")									Case SHOWDJ					Response.Write("<a class = emblem>S</a>&nbsp;")									Case OTHER					Response.Write("<a class = emblem>O</a>&nbsp;")								End Select				%>				<a href = "events.asp?style=detail&eventid=<% = objRSEvent("EventID") %>" class="weekview">				<%				Response.Write(objRSEvent("EventName"))				Response.Write("</a>")				Response.Write(" @<br>")				Response.Write(objRSEvent("VenueName"))				If Not objRSEvent.EOF Then					objRSEvent.MoveNext					intIsFirst = 0				End If				If objRSEvent.EOF Then					Exit Do				End If			Loop		End If	End If	intPrintDay = intPrintDay + 1	intIsFirst = 1End If%><p></td>      	<% Next %>    	</tr>	<% Wend %>	</table></td></tr></table></td></table><% objRSEvent.CloseEnd Sub	'MonthView()''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''''Sub WeekView()	'GetEventSet ,datFirstDay,DateAdd("d",-1,(DateAdd("m",1,datFirstDay))),APPROVED,""	%><table border=0 width="566"><tr><td>		<TABLE border = 1>	<tr>	<td align = "center" colspan = 7>	<table border = 0>	<tr>		<td>			<a href="events.asp?style=week&year=<%= Year(datLastWeek) %>&month=<%= Month(datLastWeek) %>&day=<%= Day(datLastWeek) %>"><img src="../images/left.gif" border="0" WIDTH="36" HEIGHT="36"></a>		</td>		<td>			<p class="big" align="center">WE